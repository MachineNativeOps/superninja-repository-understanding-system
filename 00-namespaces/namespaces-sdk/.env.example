# SDK Configuration
SDK_ENVIRONMENT=development
SDK_DEBUG=true

# Logging
SDK_LOG_LEVEL=info
SDK_LOG_FORMAT=json

# Tracing
SDK_TRACING_ENABLED=true
SDK_TRACING_SAMPLING_RATE=1.0

# Metrics
SDK_METRICS_ENABLED=true
SDK_METRICS_PREFIX=namespace_sdk

# Audit
SDK_AUDIT_ENABLED=true
SDK_AUDIT_RETENTION_DAYS=90

# Credentials - Environment Provider
SDK_GITHUB_TOKEN=your_github_token_here
SDK_CLOUDFLARE_API_KEY=your_cloudflare_api_key_here
SDK_OPENAI_API_KEY=your_openai_api_key_here
SDK_GOOGLE_API_KEY=your_google_api_key_here

# Credentials - File Provider
SDK_CREDENTIALS_FILE_PATH=./credentials.json

# Credentials - Vault Provider
SDK_VAULT_URL=https://vault.example.com
SDK_VAULT_TOKEN=your_vault_token_here
SDK_VAULT_NAMESPACE=your_namespace

# Credentials - AWS Secrets Manager
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# Credentials - GCP Secret Manager
GCP_PROJECT_ID=your_gcp_project_id
GOOGLE_APPLICATION_CREDENTIALS=./gcp-credentials.json

# Credentials - Azure Key Vault
AZURE_VAULT_URL=https://your-vault.vault.azure.net
AZURE_TENANT_ID=your_tenant_id
AZURE_CLIENT_ID=your_client_id
AZURE_CLIENT_SECRET=your_client_secret

# Plugin Configuration
SDK_PLUGINS_AUTO_LOAD=false
SDK_PLUGINS_DIRECTORIES=./plugins